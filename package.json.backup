{
  "name": "luxury-real-estate-app",
  "version": "1.0.0",
  "description": "Premium luxury real estate web application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:optimized": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "convert-images": "node convert-to-webp.js",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless\"",
    "perf": "npm run build && npm run lighthouse",
    "migrate-passwords": "ts-node src/scripts/migrate-passwords.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate-images": "node scripts/validate-images.js",
    "backup-images": "ts-node -e \"import('./src/lib/imageBackup').then(m => m.imageBackupManager.createBackup('Manual backup'))\"",
    "clear-cache": "ts-node -e \"import('./src/lib/imageBackup').then(m => m.imageBackupManager.clearImageCache())\"",
    "safe-deploy": "npm run validate-images && npm run backup-images && npm run build",
    "vercel-build": "npm run build:optimized",
    "postinstall": "npm run optimize-deps",
    "optimize-deps": "node scripts/optimize-deps.js",
    "prepare-vercel": "node scripts/prepare-for-vercel.js",
    "deploy-vercel": "npm run prepare-vercel && vercel --prod"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "typescript": "^5.0.0",
    "sharp": "^0.34.3",
    "lucide-react": "^0.292.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "recharts": "^3.1.2",
    "framer-motion": "^10.16.0",
    "zxcvbn": "^4.4.2",
    "argon2": "^0.44.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "mongoose": "^8.0.0",
    "formidable": "^3.5.0"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "critters": "^0.0.23",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}